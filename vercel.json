{
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "ELEVENLABS_API_KEY": "@elevenlabs-api-key",
    "BLOB_READ_WRITE_TOKEN": "@blob-read-write-token"
  },
  "build": {
    "env": {
      "ELEVENLABS_API_KEY": "@elevenlabs-api-key",
      "BLOB_READ_WRITE_TOKEN": "@blob-read-write-token"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}